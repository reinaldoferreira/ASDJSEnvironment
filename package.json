{
  "name": "asd-js-environment",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel nsp open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src build --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "nsp": "nsp check",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/data.json --port 3030",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reinaldoferreira/ASDJSEnvironment.git"
  },
  "author": "Reinaldo Ferreira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reinaldoferreira/ASDJSEnvironment/issues"
  },
  "homepage": "https://github.com/reinaldoferreira/ASDJSEnvironment#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.1.0",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.8.0",
    "json-schema-faker": "^0.3.6",
    "json-server": "^0.10.0",
    "mocha": "^3.3.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "open": "0.0.5",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.8",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5",
    "whatwg-fetch": "^2.0.3"
  }
}
